<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="159dp"
        android:layout_height="37dp"
        android:text="Oprett handleliste"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="44dp" />

    <CheckBox
        android:id="@+id/cb1OprettHandleliste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Melk"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="106dp" />

    <CheckBox
        android:id="@+id/cb2OprettHandleliste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Smør"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="147dp" />

    <CheckBox
        android:id="@+id/cb3OprettHandleliste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kjøttdeig"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="189dp" />

    <CheckBox
        android:id="@+id/cb4OprettHandleliste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Skriv for å legge til"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="230dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:srcCompat="@android:drawable/ic_delete"
        tools:layout_editor_absoluteX="332dp"
        tools:layout_editor_absoluteY="650dp" />

    <Button
        android:id="@+id/btnTilbakeOprettHandleliste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tilbake"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="23dp" />
</androidx.constraintlayout.widget.ConstraintLayout>